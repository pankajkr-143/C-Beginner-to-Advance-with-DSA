{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "C:/mingw-w64/bin/gdb.exe",
            "logging": {
                "engineLogging": true
            },
            "targetArchitecture": "x86_64",
            "pipeTransport": {
                "debuggerPath": "/usr/bin/gdb",
                "pipeCwd": "",
                "pipeProgram": ""
            }
        }
    ]
}
